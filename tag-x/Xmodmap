! For my Kinesis Ergo
clear Mod1
keycode 64 = Alt_L
keycode 134 = Alt_R
add mod1 = Alt_L Alt_R

! Use Super for dmw modkey
clear Mod4
clear lock

!! CapsLock
keycode 66 = Super_L
!! Home
keycode 110 = Super_L
!! End
keycode 115 = Super_R
add Mod4 = Super_L Super_R


!! https://bugs.debian.org/cgi-bin/bugreport.cgi?bug=564327
!! http://unix.stackexchange.com/questions/4485/reassign-ctl-and-alt-keys-xmodmap-error
!! 
!! clear Mod1
!! clear Mod4
!! keycode 204 = NoSymbol NoSymbol NoSymbol NoSymbol
!! keycode 206 = NoSymbol NoSymbol NoSymbol NoSymbol
!! keycode 64 = Alt_L Alt_L Alt_L Alt_L 
!! keycode 134 = Alt_R Alt_R Alt_R Alt_R
!! add Mod1 = Alt_L Alt_R
